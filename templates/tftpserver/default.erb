prompt 0
timeout 0
default eDeploy
serial 0

LABEL eDeploy
       KERNEL vmlinuz
       INITRD initrd.pxe
       APPEND <% if @serv -%>SERV=<%= @serv %> <% end -%>
<% if @enable_rsync -%>
<% if @rserv -%>RSERV=<%= @rserv %> <% end -%>
<% if @rserv_port -%>RSERV_PORT=<%= @rserv_port %> <% end -%>
<% end -%>
<% if @hserv -%>HSERV=<%= @hserv %> <% end -%>
<% if @hserv_port -%>HSERV_PORT=<%= @hserv_port %> <% end -%>
<% if @onfailure -%>ONFAILURE=<%= @onfailure %> <% end -%>
<% if @onsuccess -%>ONSUCCESS=<%= @onsuccess %> <% end -%>
<% if @verbose -%>VERBOSE=<%= @verbose %> <% end -%>
<% if @upload_log -%>UPLOAD_LOG=<%= @upload_log %> <% end -%>
<% if @http_path -%>HTTP_PATH=<%= @http_path %> <% end -%>
<% if @http_port -%>HTTP_PORT=<%= @http_port %> <% end -%>
