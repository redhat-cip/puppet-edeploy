prompt 0
timeout 0
default eDeploy
serial 0

LABEL eDeploy
       KERNEL vmlinuz
       INITRD initrd.pxe
       APPEND <% if @boot_conf['serv'] -%>SERV=<%= @boot_conf['serv'] %> <% end -%>
<% if @rsync_enable -%>
<% if @boot_conf['rserv'] -%>RSERV=<%= @boot_conf['rserv'] %> <% end -%>
<% if @boot_conf['rserv_port'] -%>RSERV_PORT=<%= @boot_conf['rserv_port'] %> <% end -%>
<% end -%>
<% if @boot_conf['hserv'] -%>HSERV=<%= @boot_conf['hserv'] %> <% end -%>
<% if @boot_conf['hserv_port'] -%>HSERV_PORT=<%= @boot_conf['hserv_port'] %> <% end -%>
<% if @boot_conf['onfailure'] -%>ONFAILURE=<%= @boot_conf['onfailure'] %> <% end -%>
<% if @boot_conf['onsuccess'] -%>ONSUCESS=<%= @boot_conf['onsuccess'] %> <% end -%>
<% if @boot_conf['verbose'] -%>VERBOSE=<%= @boot_conf['verbose'] %> <% end -%>
<% if @boot_conf['upload_log'] -%>UPLOAD_LOG=<%= @boot_conf['upload_log'] %> <% end -%>
<% if @boot_conf['http_path'] -%>HTTP_PATH=<%= @boot_conf['http_path'] %> <% end -%>
<% if @boot_conf['http_port'] -%>HTTP_PORT=<%= @boot_conf['http_port'] %> <% end -%>
